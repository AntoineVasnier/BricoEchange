{% extends 'BricoEchangeFrontBundle::general.html.twig' %}

{% block title %}
    {{ parent() }} - Ajout catégorie
{% endblock %}

{% block body %}
    <h2>Ajouter une nouvelle catégorie :</h2>
    <hr />
    <div style="margin: 15px 0">
    {{ form_start(form)}}
    <fieldset>
        {% form_theme form 'BricoEchangeFrontBundle:Admin:formAjoutCategorie.html.twig'%}
        {{form(form)}}
    </fieldset>
    {{ form_end(form)}}
    </div>
    
{% endblock %}